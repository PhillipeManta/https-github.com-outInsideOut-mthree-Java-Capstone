Controller(s) Mappings: 

	@GetMapping("BlogPosts")
	getAllPosts(DateTime dt) // returns: where post.dateTime > now - dt;

	@GetMapping("GetStaticPage")
	getStaticPage(pageId) 

	@PostMapping("PutBlogTrust")
	putBlogPost(blogPost)

	@PostMapping("PutStaticPage")
	putStaticPost()

	@GetMapping("editBlogPost")
	editBlogPost() // changing: posted from 0 --> 1

	@GetMapping("deleteBlogPost")
	removeBlogPost()

	@GetMapping("allPageTitles")
	getAllStaticPageTitles()

	@GetMapping("userDetails")
	getUserDetails(userName)

